    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next temp-users*.json"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.17",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.3",
    "next": "15.4.6",
    "next-auth": "^4.24.7",
    "prisma": "^5.15.0",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chart.js": "^2.9.41",
    "@types/node": "^20.12.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5"
